<h1>Listing goal_updates</h1>

<table>
  <thead>
    <tr>
      <th>Goal</th>
      <th>Entry</th>
      <th>Update date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @goal_updates.each do |goal_update| %>
      <tr>
        <td><%= goal_update.goal_id %></td>
        <td><%= goal_update.entry %></td>
        <td><%= goal_update.update_date %></td>
        <td><%= link_to 'Show', goal_update %></td>
        <td><%= link_to 'Edit', edit_goal_update_path(goal_update) %></td>
        <td><%= link_to 'Destroy', goal_update, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Goal update', new_goal_update_path %>
